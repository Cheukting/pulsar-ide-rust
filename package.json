{
  "name": "atom-ide-clangd",
  "version": "1.0.0",
  "description": "A clangd language server plugin for Atom",
  "main": "lib/main.js",
  "author": "Joshua Breeden <jbreeden@me.com>",
  "license": "MIT",
  "engines ": {
    "atom": ">=1.21.0"
  },
  "configSchema": {
  "clangExecutable": {
    "type": "string",
    "default": "/usr/local/bin/clangd",
    "description": "Absolute path to `clangd` executable."
  }
},
  "dependencies": {
    "atom-languageclient": "^0.6.1",
    "decompress": "^4.2.0"
  },
  "enhancedScopes": [
    "source.c",
    "source.cpp"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    }
  }
}
